{
    "name": "improwised/laravel-log-enhancer",
    "description": "Make debugging easier by adding more data to your laravel logs",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "monolog/monolog": "^1.23|^2.1.1|^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.20",
        "orchestra/testbench": "^6.23.0|^7.0.0|^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Improwised Technologies Pvt. Ltd.",
            "email": "info@improwised.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Improwised\\LaravelLogEnhancer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Improwised\\LaravelLogEnhancer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Improwised\\LaravelLogEnhancer\\LaravelLogEnhancerServiceProvider"
            ]
        }
    }
}
